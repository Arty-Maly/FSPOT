
<!-- header -->
<%= render 'main_page/header' %>

<!-- Main Content -->
<div class="container">

    <%= form_for User.new do |f| %>
      <%= f.text_field :email, class: "form-control", placeholder: "username"  %><br>
      <%= f.password_field :password, class: "form-control", placeholder: "password"  %><br>
      <%= f.password_field :password_confirmation, class: "form-control", placeholder: "password_confirmation"  %>
      <br>
      <%= submit_tag "Sign Up", class: "btn btn-lg btn-primary btn-block" %>
    <% end %>

</div>

<!-- Footer -->



